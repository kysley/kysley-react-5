{
  "name": "kysley-react-5",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "keywords": [],
  "author": "Evan Kysley",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --config webpack.dev.config.babel.js",
    "build": "webpack --mode production --progress --config webpack.prod.config.babel.js",
    "test": "jest --coverage --env=jsdom",
    "test:watch": "jest --coverage --env=jsdom --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "facepaint": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^4.6.5",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@types/react": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-typescript": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "raw-loader": "^1.0.0",
    "react-testing-library": "^5.8.0",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^1.2.2",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-stylish": "^0.1.8"
  },
  "jest": {
    "moduleNameMapper": {
      "^Components$": "<rootDir>/src/components/$1",
      "^Pages$": "<rootDir>/src/pages/$1",
      "^Styled$": "<rootDir>/src/styled/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "modulePaths": [
      "src"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)",
      "**/__tests__/**/*.+(js|jsx|ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "__tests__/helpers/",
      "__mocks__"
    ],
    "collectCoverageFrom": [
      "src/**/*.+(js|jsx|ts|tsx|)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "__tests__/helpers/",
      "__mocks__"
    ]
  }
}
